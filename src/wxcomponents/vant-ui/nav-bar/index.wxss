@import '../common/index.wxss';.van-nav-bar{background-color:var(--nav-bar-background-color,#fff);box-sizing:initial;height:var(--nav-bar-height,46px);line-height:var(--nav-bar-height,46px);position:relative;text-align:center;-webkit-user-select:none;user-select:none}.van-nav-bar__content{height:100%;position:relative}.van-nav-bar__text{color:var(--nav-bar-text-color,rgba(0,0,0,.9));display:inline-block;margin:0 calc(var(--padding-md, 16px)*-1);padding:0 var(--padding-md,16px);vertical-align:middle}.van-nav-bar__text--hover{background-color:#eee}.van-nav-bar__arrow{height:var(--nav-bar-arrow-size,24px);vertical-align:middle;width:var(--nav-bar-arrow-size,24px)}.van-nav-bar__arrow+.van-nav-bar__text{margin-left:-20px;padding-left:25px}.van-nav-bar--fixed{left:0;position:fixed;top:0;width:100%}.van-nav-bar--transparent{background-color:initial!important}.van-nav-bar--transition{transition:all .4s ease-in-out}.van-nav-bar__title{color:var(--nav-bar-title-text-color,rgba(0,0,0,.9));font-size:var(--nav-bar-title-font-size,17px);font-weight:var(--nav-bar-title-font-weight,600);margin:0 auto;max-width:var(--nav-bar-title-max-width,60%)}.van-nav-bar__left,.van-nav-bar__right{align-items:center;bottom:0;display:flex;font-size:var(--font-size-md,14px);position:absolute;top:0}.van-nav-bar__left{left:var(--padding-md,16px)}.van-nav-bar__left-icon-block{align-items:center;background:hsla(0,0%,100%,.45);border-radius:50%;display:flex;height:32px;justify-content:center;width:32px}.van-nav-bar__right{right:var(--padding-md,16px)}