  background-color: currentColor;
}

.color-select-block {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx 30rpx;
}

.color-select-block .border-block {
  border: #ebedf0 1px solid;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 8rpx;
  border-radius: 8rpx;
  height: 64rpx;
  font-size: 28rpx;
  color: #333333;
  width: 100%;
  margin-right: 30rpx;
}

.hex-border-block {
  min-width: 250rpx;
}

.color-select-block .border-block:last-child {
  margin: 0;
}

.color-picker-preset-color-wrap {
  display: flex;
  justify-content: space-around;
  padding: 20rpx 30rpx 30rpx;
}

.color-picker-preset-color-wrap .color-picker-preset-color {
  height: 50rpx;
  width: 50rpx;
  border-radius: 8rpx;
  border: #ebedf0 1px solid;
}

.color-picker-btns {
  display: flex;
  justify-content: center;
  align-items: center;
  border-top: 20rpx solid #f7f8fa;
  background: #fff;
}

.color-picker-btns .btn-cancel,
.color-picker-btns .btn-confirm {
  font-size: 34rpx;
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #666666;
}

.color-picker-btns .btn-confirm {
  color: #ffffff;
  background: #3A85F4;
}